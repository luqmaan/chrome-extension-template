{
  "name": "Whistlecat",
  "version": "0.2.7",
  "description": "Octocat meets whistlepig",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": [
    "http://yahoogle.com",
    "https://yahoogle.com"
  ],
  "manifest_version": 2,
  "web_accessible_resources": ["content_script.js.map"],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
